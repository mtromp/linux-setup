# .bash_PS1

#    0 – Black
#    1 – Red
#    2 – Green
#    3 – Yellow
#    4 – Blue
#    5 – Magenta
#    6 – Cyan
#    7 – White

# foreground: 30
# background: 40

# export PS1="\[\e[1;32;44m\]\u@\H\[\e[0;34m\]\W \@\$ \[\e[0m\]"
export PS1='\[\e[1;36;44m\][\u@\h \W$(__git_ps1 " (%s)") \t]\[\e[0m\]\$ '

